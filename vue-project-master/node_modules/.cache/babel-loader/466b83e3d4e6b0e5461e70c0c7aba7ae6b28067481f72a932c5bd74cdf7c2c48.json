iv>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Modal',\r\n  props: {\r\n    id: String,\r\n    type: String,\r\n    message: String,\r\n    yesButton: String,\r\n    action: Function,\r\n    modalTitle: String,\r\n    modalSubTitle: String,\r\n    modelValue: String,\r\n    classValue: String,\r\n    valid: Boolean,\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AA6BA,eAAe;EACbA,IAAI,EAAE,OAAO;EACbC,KAAK,EAAE;IACLC,EAAE,EAAEC,MAAM;IACVC,IAAI,EAAED,MAAM;IACZE,OAAO,EAAEF,MAAM;IACfG,SAAS,EAAEH,MAAM;IACjBI,MAAM,EAAEC,QAAQ;IAChBC,UAAU,EAAEN,MAAM;IAClBO,aAAa,EAAEP,MAAM;IACrBQ,UAAU,EAAER,MAAM;IAClBS,UAAU,EAAET,MAAM;IAClBU,KAAK,EAAEC;EACT;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}