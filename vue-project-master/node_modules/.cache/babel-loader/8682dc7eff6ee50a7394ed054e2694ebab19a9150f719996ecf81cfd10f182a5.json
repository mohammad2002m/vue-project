ore:hover {\n  cursor: pointer;\n}\n \n@media (max-width: 850px) {\n  td:nth-child(5),\n  th:nth-child(5) {\n    display: none;\n  }\n}\n \n@media (max-width: 576px) {\n  td:nth-child(3),\n  th:nth-child(3) {\n    display: none;\n  }\n  td:nth-child(4),\n  th:nth-child(4) {\n    display: none;\n  }\n  .title {\n    display: none;\n  }\n}\n \n@media (min-width: 760px) {\n  td, th{\n    padding: 1rem;\n  }\n}\n\n\n.see-more {\n  background-color: transparent;\n  color: #2581DD;\n  border: none;\n}\n\n.row-data td:hover {\n  background: rgba(62, 58, 58, 0.0542);\n}\n\n \n</style>"],"mappings":";AAkDA,eAAe;EACbA,IAAI,EAAE,MAAM;EACZC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAE;MACV,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,aAAa;IACjC,CAAC;IACDC,WAAWA,CAACC,KAAK,EAAC;MAChBC,OAAO,CAACC,GAAG,CAACF,KAAK;MACjB,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAACJ,IAAI,EAAE,aAAY;QAAIS,KAAK,EAAE;UAACC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,aAAa,CAACP,KAAK,CAAC;QAAC;MAAC,CAAC,GAAE;MACpGC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACK,aAAa,CAACP,KAAK,CAAC,CAAC;IACxC;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,aAAa,EAAEF,IAAI,CAACI,KAAK,CAACC,YAAY,CAACC,QAAQ,CAAC,CAACJ,aAAa;MAC9DK,QAAQ,EAAEF,YAAY,CAACE;IACzB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}