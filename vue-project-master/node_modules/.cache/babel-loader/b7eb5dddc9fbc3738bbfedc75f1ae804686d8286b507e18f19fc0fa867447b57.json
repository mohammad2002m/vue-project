In;\r\n        this.$refs.management.userInput = info.management;\r\n        this.$refs.fromDate.userInput = info.fromDate;\r\n        this.$refs.toDate.userInput = info.toDate;\r\n        this.$refs.excuse.userInput = info.excuse;\r\n        */\r\n        // this.formReadOnly = true;\r\n      } else {\r\n        // this.formReadOnly = false;\r\n      }\r\n  },\r\n  methods: {\r\n    SubmitButton() {\r\n        /*\r\n        this.$refs.firstName.CheckValidityForText();\r\n        this.$refs.secondName.CheckValidityForText();\r\n        this.$refs.mobileNumber.CheckValidityForText();\r\n        this.$refs.holidayType.CheckValidityForSelect();\r\n        this.$refs.outOrIn.CheckValidityForSelect();\r\n        this.$refs.management.CheckValidityForText();\r\n        this.$refs.fromDate.CheckValidityForDate();\r\n        this.$refs.toDate.CheckValidityForDate();\r\n        this.$refs.excuse.CheckValidityForText();\r\n        */\r\n      if (\r\n        this.$refs.firstName.CheckValidityForText() & \r\n        this.$refs.secondName.CheckValidityForText() &\r\n        this.$refs.mobileNumber.CheckValidityForText() & \r\n        this.$refs.holidayType.CheckValidityForSelect() &\r\n        this.$refs.outOrIn.CheckValidityForSelect() &\r\n        this.$refs.management.CheckValidityForText() & \r\n        this.$refs.fromDate.CheckValidityForDate() & \r\n        this.$refs.toDate.CheckValidityForDate() & \r\n        this.$refs.excuse.CheckValidityForText()\r\n\r\n      ) {\r\n\r\n        let request = {\r\n          firstName: this.$refs.firstName.userInput,\r\n          secondName: this.$refs.secondName.userInput,\r\n          mobileNumber: this.$refs.mobileNumber.userInput,\r\n          holidayType: this.$refs.holidayType.userInput,\r\n          outOrIn: this.$refs.outOrIn.userInput,\r\n          management: this.$refs.management.userInput,\r\n          fromDate: this.$refs.fromDate.userInput,\r\n          toDate: this.$refs.toDate.userInput,\r\n          excuse: this.$refs.