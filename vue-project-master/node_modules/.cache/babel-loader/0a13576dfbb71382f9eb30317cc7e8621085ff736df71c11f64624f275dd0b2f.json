n  data() {\r\n    return {\r\n      requestsArray: JSON.parse(localStorage.requests).requestsArray,\r\n      userType: localStorage.userType,\r\n    }\r\n  },\r\n  mounted:{\r\n    isManager(){\r\n      return this.Database.getCurrentUser().role === 'manager';\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\ntable {\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n  text-align: center;\r\n}\r\n\r\ntd,\r\nth {\r\n  border: 1px solid rgba(147, 139, 139, 0.593);\r\n  padding: 20px;\r\n  text-align: center;\r\n}\r\n\r\n\r\n.see-more:hover {\r\n  cursor: pointer;\r\n}\r\n\r\n@media (max-width: 850px) {\r\n\r\n  td:nth-child(5),\r\n  th:nth-child(5) {\r\n    display: none;\r\n  }\r\n}\r\n\r\n@media (max-width: 576px) {\r\n\r\n  td:nth-child(3),\r\n  th:nth-child(3) {\r\n    display: none;\r\n  }\r\n\r\n  td:nth-child(4),\r\n  th:nth-child(4) {\r\n    display: none;\r\n  }\r\n\r\n  .title {\r\n    display: none;\r\n  }\r\n}\r\n\r\n@media (min-width: 760px) {\r\n\r\n  td,\r\n  th {\r\n    padding: 1rem;\r\n  }\r\n}\r\n\r\n\r\n.see-more {\r\n  background-color: transparent;\r\n  color: #2581DD;\r\n  border: none;\r\n}\r\n\r\n.row-data td:hover {\r\n  background: rgba(62, 58, 58, 0.0542);\r\n}</style>"],"mappings":";AAmDA,eAAe;EACbA,IAAI,EAAE,MAAM;EACZC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,aAAa;IACjC,CAAC;IACDC,WAAWA,CAACC,KAAK,EAAE;MACjBC,OAAO,CAACC,GAAG,CAACF,KAAK;MACjB,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAAEJ,IAAI,EAAE,aAAa;QAAES,KAAK,EAAE;UAAEC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACC,aAAa,CAACP,KAAK,CAAC;QAAE;MAAE,CAAC,GAAE;MACvGC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACK,aAAa,CAACP,KAAK,CAAC,CAAC;IACxC;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,aAAa,EAAEF,IAAI,CAACI,KAAK,CAACC,YAAY,CAACC,QAAQ,CAAC,CAACJ,aAAa;MAC9DK,QAAQ,EAAEF,YAAY,CAACE;IACzB;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,SAASA,CAAA,EAAE;MACT,OAAO,IAAI,CAACC,QAAQ,CAACC,cAAc,EAAE,CAACC,IAAG,KAAM,SAAS;IAC1D;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}