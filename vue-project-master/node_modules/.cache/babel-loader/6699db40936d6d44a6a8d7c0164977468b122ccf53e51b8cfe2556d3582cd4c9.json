tWithId(requestId).toDate;\r\n\r\n      }\r\n    else {\r\n      console.log('No');\r\n    }\r\n  },\r\n  methods: {\r\n    test() {\r\n      console.log('test')\r\n    },\r\n    sendRequest() {\r\n      const newVacation = this.vacationType;\r\n      const newToDate = this.toDate;\r\n      const newFromDate = this.fromDate;\r\n      const newExcuse = this.excuse;\r\n      const newInOrOut = this.inOrOut;\r\n\r\n      let newRequest = {\r\n        holidayType: newVacation,\r\n        management: this.Database.getRequestWithId(this.Database.getCurrentUserId()).management,\r\n        fromDate: newFromDate,\r\n        toDate: newToDate,\r\n        status: 'جديد',\r\n        inOrOut: newInOrOut,\r\n        userId: this.Database.getCurrentUserId(),\r\n        holidayReason: newExcuse,\r\n        requestId: Math.floor(Math.random() * 10000),\r\n      };\r\n      this.Database.addRequest(newRequest);\r\n      const bsAlert = new bootstrap.Alert('#myAlert')\r\n      console.log(bsAlert)\r\n\r\n    },\r\n    classValue(dirty , invalid){\r\n      // console.log(this.fromDate , this.toDate)\r\n      // console.log(dirty , invalid)\r\n      if (!dirty){\r\n        return '';\r\n      } else if (invalid){\r\n        return 'is-invalid';\r\n      } else return 'is-valid';\r\n  },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ndiv {\r\n  direction: rtl;\r\n}\r\n\r\n.form-title {\r\n  display: block;\r\n  text-align: center;\r\n  font-size: 22px;\r\n  margin-bottom: 3rem;\r\n}\r\n\r\n.box {\r\n  background-color: white;\r\n  margin-bottom: 50px;\r\n}\r\n\r\nbutton {\r\n  margin-right: 5px;\r\n  margin-left: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AAmDA,OAAOA,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,gBAAe,MAAO,wCAAwC;AACrE,OAAOC,SAAQ,MAAO,iCAAiC;AACvD,OAAOC,WAAU,MAAO,mCAAmC;AAC3D,OAAOC,WAAU,MAAO,mCAAmC;AAC3D,OAAOC,SAAQ,MAAO,iCAAiC;AACvD,OAAOC,KAAI,MAAO,oBAAoB;AACtC,OAAOC,aAAY,MAAO,qCAAqC;AAC/D,OAAOC,gBAAe,MAAO,+BAA+B;AAC5D,SAASC,YAAY,EAAEC,kBAAiB,QAAS,uBAAsB;AACvE,OAAOC,MAAK,MAAO,QAAQ;AAC3